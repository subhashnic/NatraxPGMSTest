@{
    ViewBag.Title = "Wet Skid Pad Detail";
}

<style>
    .card .card-header .card-title {
        font-weight: 700;
        margin-bottom: 0px;
        border-bottom: 1px solid #ddd;
        padding: 25px 10px 5px 0px;
        margin: 0;
        box-shadow: none !important;
        background: #ffffff;
    }
</style>
<div class="content">
    <div class="container-fluid">
        <div class="home-demo">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <h4 class="card-title">Wet Skid Pad</h4>
                        </div>
                        <div class="card-body">
                            <div id="accordion" role="tablist">
                                <div class="card-collapse">
                                    <div class="card-header" role="tab" id="headingTwo">
                                        <h5 class="mb-0">
                                            <a class="collapsed" data-toggle="collapse" href="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">
                                                Description & Documents
                                                <i class="material-icons">keyboard_arrow_down</i>
                                            </a>
                                        </h5>
                                    </div>
                                    <div id="collapseTwo" class="collapse show" role="tabpanel" aria-labelledby="headingTwo" data-parent="#accordion" style="">
                                        <div class="card-body">
                                            <div class="row">
                                                <div class="col-md-9">
                                                    <h4>Technical Specification:</h4>
                                                    <p>Circular pad to develop ESP systems and tyres in wet conditions. Includes two lanes with different adhesion coefficient: basalt and asphalt.</p>
                                                    <h4>Use of Track:</h4>
                                                    <p>The wet skid pad is a track with a low grip in which tests of TCS, development of tyres and electronics can be performed.</p>
                                                </div>
                                                <div class="col-md-3">
                                                    <a href="javascript:void(0)" style="color: #000;"><img src="~/Templates/img/pdfIcon.png">PDF for detail</a><br />
                                                    <a href="javascript:void(0)" style="color: #000;"><img src="~/Templates/img/pdfIcon.png">PDF for detail</a><br />
                                                    <a href="javascript:void(0)" style="color: #000;"><img src="~/Templates/img/pdfIcon.png">PDF for detail</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="card-collapse">
                                    <div class="card-header" role="tab" id="headingOne">
                                        <h5 class="mb-0">
                                            <a data-toggle="collapse" href="#collapseOne" aria-expanded="false" aria-controls="collapseOne" class="">
                                                Images
                                                <i class="material-icons">keyboard_arrow_down</i>
                                            </a>
                                        </h5>
                                    </div>
                                    <div id="collapseOne" class="collapse" role="tabpanel" aria-labelledby="headingOne" data-parent="#accordion" style="">
                                        <div class="card-body">
                                            <div class="owl-carousel owl-theme">
                                                <div class="item">
                                                    <img class="img-responsive img-thumbnail" src="~/Templates/Tracks/T-11 Wet Skid Pad.jpg" width="300" height="200" />
