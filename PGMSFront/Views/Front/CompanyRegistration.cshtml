@{
    ViewBag.Title = "Company Registration";
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="~/Templates/assets/img/apple-icon.png">
    <link rel="icon" type="image/png" href="~/Templates/assets/img/favicon.png">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>
        @ViewBag.Title
    </title>
    <meta content='width=device-width, initial-scale=1.0, shrink-to-fit=no' name='viewport' />
    <link rel="canonical" href="https://www.creative-tim.com/product/material-dashboard-pro" />
    <meta name="keywords" content="PGMS">
    <meta name="description" content="PGMS">
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
    <link href="~/Templates/assets/demo/demo.css" rel="stylesheet" />
    <link href="~/Templates/assets/css/material-dashboard.css?v=2.1.0" rel="stylesheet" />
    <style>
        .register-page .card-signup {
            margin-top: 0;
            margin-bottom: 50px;
            padding: 10px 0px;
        }

        .form-control {
            height: 30px !important;
        }

        .off-canvas-sidebar .wrapper-full-page .page-header {
            padding: 5vh 0 !important;
        }

        .register-page .card-signup .form-check {
            margin-top: 20px;
            margin-bottom: 0;
        }

        .card-signup .form-check {
            padding-top: 10px;
        }

        .form-check-label {
            padding-top: 2px;
        }
    </style>
</head>

<body class="off-canvas-sidebar">
    <div class="wrapper wrapper-full-page">
        <div class="page-header register-page header-filter" filter-color="black" style="        background-image: url('../Templates/Tracks/Natrax Overall Track Pics.jpg')">
            <div class="container">
                <div class="row">
                    <div class="col-md-7 ml-auto mr-auto">
                        <div class="card card-signup" style="        padding: 0px 0px;">
                            <div class="col-md-6 ml-auto mr-auto" style="        text-align: center;
        max-width: 56%;">
                                <img src="~/Templates/img/logo.png" alt="">
                            </div>
                            <h2 class="card-title text-center" style="        font-size: 20px;
        margin-bottom: 0px;">
                                Company Registration
                            </h2>
                            <div class="card-body">
                                <div class="row">
                                    <div class="card-body">
                                        <div class="col-md-12 ml-auto">
                                            <form>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="form-group bmd-form-group">
                                                            <label for="exampleEmail" class="bmd-label-floating">Company Name *</label>
                                                            <input type="text" class="form-control" id="txtCompanyName">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="form-group bmd-form-group">
                                                            <label for="examplePass" class="bmd-label-floating">Address *</label>
                                                            <textarea class="form-control" id="txtAddress"></textarea>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <div class="form-group bmd-form-group">
                                                            <label for="examplePass" class="bmd-label-floating"></label>
                                                            <select class="selectpicker" data-size="7" data-style="select-with-transition" title="Select State *">
                                                                <option>MP</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <div class="form-group bmd-form-group">
                                                            <label for="examplePass" class="bmd-label-floating"></label>
                                                            <select class="selectpicker" data-size="7" data-style="select-with-transition" title="Select City *">
                                                                <option>Indore</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="form-group bmd-form-group">
                                                            <label for="examplePass" class="bmd-label-floating">Pincode *</label>
                                                            <input type="text" class="form-control" id="txtPincode">
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group bmd-form-group">
                                                            <label for="examplePass" class="bmd-label-floating">
                                                                GST No.
                                                            </label>
                                                            <input type="text" class="form-control" id="txtGSTNo">
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group bmd-form-group">
                                                            <label for="examplePass" class="bmd-label-floating">PAN No.</label>
                                                            <input type="text" class="form-control" id="txtPANNo">
                                                        </div>
                                                    </div>
                                                </div>


                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="form-group bmd-form-group">
                                                            <label for="examplePass" class="bmd-label-floating">
                                                                How did you come to know about NATRAX
                                                            </label><br />
                                                            <div class="form-check">
                                                                <label class="form-check-label">
                                                                    <input class="form-check-input" type="radio" name="exampleRadios" value=""> TV
                                                                    <span class="circle">
                                                                        <span class="check"></span>
                                                                    </span>
                                                                </label>
                                                                <label class="form-check-label">
                                                                    <input class="form-check-input" type="radio" name="exampleRadios" value=""> News Paper
                                                                    <span class="circle">
                                                                        <span class="check"></span>
                                                                    </span>
                                                                </label>
                                                                <label class="form-check-label">
                                                                    <input class="form-check-input" type="radio" name="exampleRadios" value=""> Internet
                                                                    <span class="circle">
                                                                        <span class="check"></span>
                                                                    </span>
                                                                </label>
                                                                <label class="form-check-label">
                                                                    <input class="form-check-input" type="radio" name="exampleRadios" value=""> Others
                                                                    <span class="circle">
                                                                        <span class="check"></span>
                                                                    </span>
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="form-group bmd-form-group">
                                                            <label for="examplePass" class="bmd-label-floating">
                                                                References, if any
                                                            </label>
                                                            <input type="text" class="form-control" id="txtRef">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="form-group bmd-form-group">
                                                            <label for="examplePass" class="bmd-label-floating"></label>
                                                            <a href="#" class="btn btn-primary btn-round mt-4 pull-right">Cancel</a>
                                                            <a href="#" class="btn btn-primary btn-round mt-4 pull-right">Register</a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            @*<footer class="footer">
                    <div class="container">
                        <nav class="float-left">
                            <ul>
                                <li>
                                    <a href="javascript:void(0)">
                                        Natrax
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)">
                                        About Us
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:void(0)">
                                        Licenses
                                    </a>
                                </li>
                            </ul>
                        </nav>
                        <div class="copyright float-right">
                            <a href="javascript:void(0)" target="_blank">Copyright &copy; 2020 All rights reserved</a>
                        </div>
                    </div>
                </footer>*@
        </div>
    </div>
    <!--   Core JS Files   -->
    <script src="~/Templates/assets/js/core/jquery.min.js"></script>
    <script src="~/Templates/assets/js/core/popper.min.js"></script>
    <script src="~/Templates/assets/js/core/bootstrap-material-design.min.js"></script>
    <script src="~/Templates/assets/js/plugins/perfect-scrollbar.jquery.min.js"></script>
    <!--  Google Maps Plugin    -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB2Yno10-YTnLjjn_Vtk0V8cdcY5lC4plU"></script>
    <!-- Place this tag in your head or just before your close body tag. -->
    <script async defer src="https://buttons.github.io/buttons.js"></script>
    <!-- Chartist JS -->
    <script src="~/Templates/assets/js/plugins/chartist.min.js"></script>
    <!--  Notifications Plugin    -->
    <script src="~/Templates/assets/js/plugins/bootstrap-notify.js"></script>
    <!-- Control Center for Material Dashboard: parallax effects, scripts for the example pages etc -->
    <script src="~/Templates/assets/js/material-dashboard.min.js?v=2.1.2" type="text/javascript"></script>
    <!-- Material Dashboard DEMO methods, don't include it in your project! -->
    <script src="~/Templates/assets/demo/demo.js"></script>
    <script src="~/Templates/assets/js/plugins/bootstrap-selectpicker.js"></script>
    <!-- Sharrre libray -->
    <script src="~/Templates/assets/demo/jquery.sharrre.js"></script>
</body>
</html>

