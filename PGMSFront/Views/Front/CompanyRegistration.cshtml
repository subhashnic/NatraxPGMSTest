                alert("Please enter your Login Id");
            }
            else {
                LoaderShow();
                var data = { 'strLoginId': $("#txtLoginId").val().trim()};
                var dataWithAntiforgeryToken = $.extend(data, { "__RequestVerificationToken": $("input[name=__RequestVerificationToken]").val() });
                $.ajax({
                    url: '@Url.Action("CheckLoginIdAvailability", "Front")',
                    type: 'POST',
                    dataType: 'json',
                    async: true,
                    data: dataWithAntiforgeryToken,
                    success: function (data) {
                        LoaderHide();
                        if (data.StatusId != "99" && data.StatusId != "-99") {

                        }
                        else {
                            $("#txtLoginId").val("");
                            alert(data.Status);
                        }
                    },
                    error: function (xhr) {

                        LoaderHide();
                        alert(xhr.responseText);
                    }
                });
            }
        }
        catch (e) {

            LoaderHide();
            alert(e.message);
            }

    }

    ///////////////////////////// Registration Save  //////////////////////////////////////////////////////
    function CompanyRegistrationSave() {
        try {
            var strMSG = '';

            var varCity = $("#ddlCity").val();
            if ($("#txtCompanyName").val().trim() == "") {
                strMSG = strMSG + 'Company Name \n</br>'
            }
            if ($("#txtAddress").val().trim() == "") {
                strMSG = strMSG + 'Address \n</br>'
            }
            if ($("#ddlState").val() == null || $("#ddlState").val()==undefined || parseInt($("#ddlState").val()) ==0) {
                strMSG = strMSG + 'State \n</br>'
            }
            if (varCity == null || varCity==undefined || parseInt(varCity) == 0) {
                strMSG = strMSG + 'District \n</br>'
            }
            if ($("#txtPincode").val().trim() == "") {
                strMSG = strMSG + 'Pincode \n</br>'
            }
            if ($("#txtEMailId").val().trim() == "") {
                strMSG = strMSG + 'EMail \n</br>'
            }
            if ($("#txtMobileNo").val().trim() == "") {
                strMSG = strMSG + 'Contact No. \n</br>'
            }
            if ($("input[name='rdoKnowAbout']:checked").val()==undefined || $("input[name='rdoKnowAbout']:checked").val() == "") {
                strMSG = strMSG + 'Know about NATRAX \n</br>'
            }
            if ($("#txtLoginId").val().trim() == "") {
                strMSG = strMSG + 'Login Id \n</br>'
            }

            if (strMSG != '') {
                alert("Please enter data for Mandatory fields  \n</br>" + strMSG);
                return false;
            }
            else {
                var model = {};

                model.CustomerMasterId = -1;
                model.CompanyName = $("#txtCompanyName").val().trim();
                model.StateId = $("#ddlState").val().trim();
                model.Address = $("#txtAddress").val();
                model.DistrictId = varCity;
                model.PinCode = $("#txtPincode").val().trim();
                model.Email = $("#txtEMailId").val().trim();
                model.ContactPerson=$("#txtContactPerson").val().trim();
                model.PhoneNo = $("#txtMobileNo").val().trim();
                model.GSTNo = $("#txtGSTNo").val().trim();
                model.PANNo = $("#txtPANNo").val().trim();
                model.Reference = $("#txtRef").val().trim();
                model.KnowAboutNatraxOL = $("input[name='rdoKnowAbout']:checked").val();
                model.ZZLoginId = $("#txtLoginId").val().trim();

                $("#btnReg").hide();
                LoaderShow();
                var data = {'model': model};
                var dataWithAntiforgeryToken = $.extend(data, { "__RequestVerificationToken": $("input[name=__RequestVerificationToken]").val() });
                $.ajax({
                    url: '@Url.Action("CompanyRegistrationSave", "Front")',
                    type: 'POST',
                    dataType: 'json',
                    async: true,
                    data: dataWithAntiforgeryToken,
                    //contentType: "application/json; charset=utf-8",
                    success: function (data) {
                       LoaderHide();
                        if (data.StatusId != "99" && data.StatusId != "-99") {
                            $("#btnReg").hide();
                            $("#btnCancel").text("BACK");
                            $("#lblRegisterMSG").text(data.Status);
                            alert(data.Status);
                        }
                        else {
                            $("#btnReg").show();
                            alert(data.Status);
                        }
                       // $("#btnReg").show();

                    },
                    error: function (xhr) {

                        $("#btnReg").show();
                        LoaderHide();
                        alert(xhr.responseText);
                    }
                });

            }
        }
        catch (e) {

            LoaderHide();
            $("#btnReg").show();
            alert(e.message);
            return false;
        }
    }



    ///////////////////////////// Masking  //////////////////////////////////////////////////////
    function MaskControls() {
        try {
            (function ($) {
                $(function () {
                    $("#txtCompanyName").attr('onkeypress', 'return CheckIsAlphaNumeric(event,this);');
                    $("#txtGSTNo").attr('onkeypress', 'return CheckIsAlphaNumeric(event,this);');
